@section jssection{
<script type="text/javascript">
    window.onload = function () {
        (function ($) {
            var handler = $('#tiles li');
            handler.wookmark({
                autoResize: true,
                container: $('#main'),
                offset: 20,
                outerOffset: 5,
                itemWidth: 300
            });

        })(jQuery);
    }
    function edit() {
        document.getElementById('tishi').style.display = "block";
        document.getElementById('tishibg').style.display = "block";
    }
    function closed() {
        document.getElementById('tishi').style.display = "none";
        document.getElementById('tishibg').style.display = "none";
    }

    function deletetw(id)
    {
        if (confirm("是否删除图文？")) {
            //$("#li" + id).remove();
            $.post("DeleteTW.do?id="+id, {
                id: id
            },
                  function (ret) {
                      if (ret && ret.status == 0) {
                          //alert(ret.data.ID);
                          location = "graphicshow.do";
                          // alert(ret.data.NAME);

                      }
                      else {
                          _showInfoMessage("删除失败！", 'error');
                      }
                  },
                  "json"
            );
        }
    }

</script>
    @Html.Js("plugins/xjgrid/xjgrid", "plugins/xjdailog/xjdailog", "plugins/validator/validator", "js/jquery.wookmark")
}
