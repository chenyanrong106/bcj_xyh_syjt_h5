                            </div>
                        </div>
                        <div class="form-group">
                            <label for="USER_PASS" class="col-sm-3 control-label"><span style="color: red">*</span>密码</label>
                            <div class="col-sm-6">
                                @Html.Password("USER_PASS", "", new { @class = "form-control", placeholder = "请输入密码" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="USER_PASS1" class="col-sm-3 control-label"><span style="color: red">*</span>确认密码</label>
                            <div class="col-sm-6">
                                @Html.Password("USER_PASS1", "", new { @class = "form-control", placeholder = "请输入确认密码" })
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" id="btnCloseDlzh" class="btn btn-default" data-dismiss="modal">取 消</button>
                            <button type="button" id="btnSavePass" class="btn btn-primary">保 存</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
