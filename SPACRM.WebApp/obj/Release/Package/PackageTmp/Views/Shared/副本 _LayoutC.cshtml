                <div class="modal-body">
                    <form class="form-horizontal" id="frmPass" method="post" action="/Employee/SavePass.do" role="form">
                        <div class="form-group">
                            <label for="USER_NO" class="col-sm-3 control-label"><span style="color: red">*</span>登录名</label>
                            <div class="col-sm-6">
                                @Html.TextBox("USER_NO", "", new { @class = "form-control", @readonly = "readonly", placeholder = "请输入登录名" })
