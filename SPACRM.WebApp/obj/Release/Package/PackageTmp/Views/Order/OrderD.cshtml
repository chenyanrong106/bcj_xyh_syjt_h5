    <div class="windo" id="windo">
        <div class="content">
            <div class="title">
                <a href="javascript:void(0)" id="closewin" class="closeBtn">关闭</a>
            </div>
            <div style="margin-left: 5px; width: 332px;">
                <input type="text" id="EMP_NAME" class="form-control" placeholder="输入姓名/工号检索" />
            </div>
            <div class="main">
                <div id="divcontent" class="divcontent">
                    <ul style="float: left; padding: 2px 0px 2px 2px;" id="ulcontent">
                        @foreach (var item in (List<ORG_EMPLOYEE_EX>)ViewBag.employeeList)
                        {
                            <li onclick="addEmp(this)" id="@item.NAME" itemid="@item.ID">
                                <div class="contentemp">
                                    <div>@item.EMPLOYEE_NO</div>
                                    <div>@item.NAME</div>
                                </div>
                            </li>
                        }

                    </ul>
                </div>

                <div id="divcontentone" class="divcontent" style="display: none;">
                    <ul style="float: left; padding: 2px 0px 2px 2px;" id="ulcontentone">
