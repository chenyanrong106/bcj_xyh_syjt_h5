<form action="@Url.Action("Save")" id="formSave" method="post" role="form">
    <div class="s_container">
        <div id="page-heading">
            <h2>
                <i class="fa fa-shopping-cart"></i>订单</h2>
        </div>

        <div class="row">
            <div class="col-md-4">
                <div class="panel" style="width: 100%">
                    <input type="hidden" id="PROD_NAME_PRICE" style="width: 100%" />
                </div>
                <div class="panel panel-primary">
                    <div id="panelService1" class="tab-container tab-gray">
                        <ul class="nav nav-tabs" id="ultab">
                            <li><a href="#spro" data-toggle="tab">常用</a></li>
                            <li class="active"><a href="#service" data-toggle="tab">服务</a></li>
                            <li><a href="#goods" data-toggle="tab">产品</a></li>
                            <li style="display: none"><a href="#treatment" data-toggle="tab">可用疗程</a></li>
                        </ul>
                        <div id="tab-content" class="tab-content">
                            <div class="tab-pane" id="spro">
                                <div class="list-group">

                                    <table id="tSpeedyPro" class="table table-advance table-hover">
                                        <tbody>
                                            @foreach (var item in (List<PROD_GOODS_EX>)ViewBag.proList)
                                            {
                                                <tr id="@item.ID" itemid="@item.TABLE">
                                                    <td>@item.NAME</td>
                                                    <td>
                                                        <input type='hidden' name='is_cpay_dist' value="@item.PRICE,@item.IS_CARDPAY.ToString(),@item.IS_DISCOUNT.ToString()" /></td>
                                                    <td class="tdPice"><span class="badge">￥@item.PRICE</span></td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="tab-pane active" id="service">
                                <div class="form-group">
                                    <div id="serGridMenu">
                                        <ul>
                                            @if (ViewBag.SerCategoryList != null)
                                            {
                                                foreach (var item in ViewBag.SerCategoryList)
                                                {
                                                <li><a onclick="util.checkSerMenu(this)" itemid='@item.ID'>@item.CATE_NAME</a></li>
                                                }
                                            }
                                        </ul>
                                    </div>
                                    <div id="serGridlist" style="height: 460px; overflow: auto; overflow-x: hidden;">
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="goods">
                                <div class="form-group">
                                    <div id="proGridMenu">
                                        <ul>
                                            @if (ViewBag.ProCategoryList != null)
                                            {
                                                foreach (var item in ViewBag.ProCategoryList)
                                                {
                                                <li><a onclick="util.checkProMenu(this)" itemid='@item.ID'>@item.CATE_NAME</a></li>
                                                }
                                            }
                                        </ul>
                                    </div>
                                    <div id="goodsGridlist" style="height: 460px; overflow: auto; overflow-x: hidden;">
                                    </div>
                                </div>
                            </div>

                            <div class="tab-pane" id="treatment">
                                <div id="treatmentlist" class="list-group" style="height: 440px; overflow: auto; overflow-x: hidden;">
                                    <table id="tTreatmentPro" class="table table-advance table-hover">
                                        <tbody>
                                            <tr>
                                                <td>暂无疗程卡信息</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <div id="divCustomer" class="col-md-8">
                <div class="panel">
                    <div class="panel-body">
                        <div class="col-md-6">
                            <div class="table-responsive">
                                <table class="table table-condensed">
                                    <tbody>
                                        <tr class="divCustomer-tr">
                                            <td style="width: 30%;">顾客姓名</td>
                                            <td>
                                                <span id="spanCustName"></span>&nbsp;<a onclick="util.updateCust()" href="javascript:;">更换顾客</a>&nbsp;<a id="filelist" href="javascript:;">查看附件</a>&nbsp;<a id="buycard" href="javascript:;">充值</a>

                                            </td>
                                        </tr>
                                        <tr>
                                            <td>联系电话</td>
                                            <td><span id="spanCustMobile"></span></td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>会员卡号</td>
                                            <td><span id="spanCustCard"></span></td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>会员卡级</td>
                                            <td><span id="spanCustCardLevel"></span></td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>会员人数</td>
                                            <td>@Html.TextBox("MEMBER_NUM", Model.MEMBER_NUM, new { @type = "number" })</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="table-responsive">
                                <table class="table table-condensed">
                                    <tbody>
                                        @if (!string.IsNullOrEmpty(Request.QueryString["oid"]))
                                        {
                                            <tr style="border-bottom: 1px solid red; border-top: 2px solid #ffffff;">

                                                <td>订单号</td>
                                                <td><span id="spanOrder_No"></span></td>

                                            </tr>
                                        }
                                        else
                                        {
                                            <tr style="border-top: 2px solid #ffffff;">
                                                <td>&nbsp;</td>
                                                <td>&nbsp;</td>
                                            </tr>  
                                        }
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>创建时间</td>
                                            <td><span id="spanOrder_Date">@DateTime.Now.ToString("yyyy年MM月dd日")</span>
                                                <input type="hidden" id="hideOrder_Date" value="@DateTime.Now.ToString()" />
                                            </td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>房&nbsp;&nbsp;&nbsp;&nbsp;间</td>
                                            <td>
                                                @Html.ResourceDropDownList("ROOM_ID", "ROOM", "", new { style = "width:120px;" }, (string)ViewBag.STORE_ID, true)

                                            </td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>卡内余额</td>
                                            <td><span id="spanCustCardBalance"></span></td>
                                        </tr>
                                        <tr style="border-bottom: 1px solid #e6e5e5;">
                                            <td>散客人数</td>
                                            <td>@Html.TextBox("WALKIN_NUM", Model.WALKIN_NUM, new { @type = "number" })</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
            <div id="divNewCustomer" class="col-md-8">
                <div class="panel">
                    <div class="panel-body2">
                        <div class="">
                            <div class="table-responsive">
                                <table class="table table-condensed">
                                    <tbody>
                                        <tr class="divNewCustomer_tr">
                                            <td style="">
                                                <span>顾客姓名</span>

                                                <input type="hidden" id="CUST_NAME" style="width: 240px;" />

                                                <a href="javasrcipt:;" id="firstname" data-type="text" data-pk="2" data-placement="right" data-title="新增顾客信息"><span id="spanCust"></span></a>
                                                <button type="button" id="btnOrderBill" class="btn btn-primary">散客开单</button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-md-8">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h4>订购项目</h4>
                    </div>
                    <div id="divloading" class="panel-body">
                        <div id="divCart">
                            <img src="~/assets/img/cart.jpg" />
                        </div>

                        <div id="divProds" style="display: none;">
                            <div style="float: left;">
                                <table id="tableProds" border="0" style="width: 100%;">
                                    <thead>
                                        <tr class="divProds_tr">
                                            <th style="width: 2%">#</th>
                                            <th style="width: 30%">项目名称</th>
                                            <th style="width: 20%; text-align: left">护理师</th>
                                            <th style="width: 10%; text-align: left">门市价</th>
                                            <th style="width: 13%; text-align: left">优惠/活动</th>
                                            <th style="width: 13%; text-align: center">最终价格</th>
                                            <th style="width: 8%"></th>
                                        </tr>
                                    </thead>
                                    <tbody style="font-size: 12px;">
                                    </tbody>
                                </table>
                            </div>

                            <div style="margin: 0 auto; position: absolute; float: left; left: 40%; top: 30%;">
                                <div id="progressImgage" style="display: none; margin: 0px auto; float: left;">
                                    <img src="~/assets/img/loading2.gif" />
                                </div>
                                <div id="maskOfProgressImage" class="mask" style="display: none;"></div>
                            </div>
                        </div>
                        <div id="divPrice">
                            <table border="0" style="width: 100%">
                                <tr style="width: 50%">
                                    <td>
                                        <table>
                                            <tr>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="alert alert-dismissable alert-warning">
                                                        <span class="fa fa-exclamation-triangle"></span>
                                                        支付提醒：<br />
                                                        必须现金支付金额：<span id="spanCashPay"></span><br />
                                                        允许卡支付但不享受折扣金额：<span id="spanCardPay">0.00</span><br />
                                                        允许卡支付并享受折扣金额：<span id="spanCardDistPay">0.00</span>
                                                    </div>
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                    <td>
                                        <table border="0" style="width: 100%">
                                            <tr style="width: 50%">
                                                <td style="text-align: right;">
                                                    <div>
                                                        门市总价： <b>¥<span id="spanMSZJ">0.00</span>   </b>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: right;">
                                                    <div id="divYH" style="width: 100px;">
                                                        优惠： ¥<span id="spanYH">0.00</span>
                                                    </div>
                                                </td>
                                            </tr>
                                            @{ string display = (ViewBag.HasServiceFee == 1) ? "" : "none"; }
                                            <tr style='display: @display;'>
                                                <td style="text-align: right;">
                                                    <div>
                                                        <input type="checkbox" id="chkFWF" name="chkFWF" />
                                                        收取服务费(@ViewBag.ServiceFeeRate)： <b style="color: blue;">¥<span id="spanFWF">0.00</span></b>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: right;">

                                                    <h3>应付：<b style="color: red">¥<span id="spanZJ">0.00</span></b></h3>
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </table>
                        </div>

                        <div class="form-group btnpanel_right">
                            <button type="button" id="btnCheckIn" style="display: none;" class="btn btn-primary">保存并支付</button>
                            <button type="button" id="btnSave" style="display: none;" class="btn btn-primary">保存订单</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

